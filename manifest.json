{
  "name": "Delicious Redux",
  "version": "0.3",
  "manifest_version": 2,
  "description": "A simpler, faster delicious bookmarking extension",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://api.del.icio.us/"
  ],
  "icons": {
    "128":"128_delicious.png"
  }
}